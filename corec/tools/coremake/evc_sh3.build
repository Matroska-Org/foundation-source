#define TARGET_WINCE
#define TARGET_WIN
#define SH3

PLATFORMLIB = wince_sh3

TARGID = WCE SH3
TARGCODE = 0x81
TARGCPU = "{D6519020-710F-11D3-99F2-00105A0DF099}"
TARGCC = shcl.exe
TARGDEFINE = STANDARDSHELL_UI_MODEL
TARGDEFINE += SH3
TARGDEFINE += _SH3_
TARGDEFINE += SHx
TARGCFLAGS =
TARGCFLAGS_REL = /Oxt
TARGLFLAGS = /MACHINE:SH3
TARGASM = shasm "$(InputPath)" -cpu=SH3 -o="$(IntDir)/$(InputName).obj"

#include "evc.inc"
