//TARGLFLAGS = /subsystem:windowsce,5.01
TARGLFLAGS = /subsystem:windowsce,3.00
TARGMACHINE = 7
TARGCOMPILEARCH = 6
TARGEXESUBSYSTEM = 9
SVNDIR=msvc8\mipsii
VSPDB=vc80.pdb
WINCE_VERSION=5.00
WINCE_CS_UUID=E2BECB1F-8C8C-41ba-B736-9BE7D946A398

TARGGCC = mipsel-linux-gnu-gcc -D_WIN32_WCE=500 -DMIPS -msoft-float -mno-explicit-relocs -fomit-frame-pointer -fsigned-char -march=mips32 -O2

#include "vs.inc"
